    "babel-tape-runner": "^2.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "jquery": "^3.2.1",
    "nsp": "^2.6.3",
    "nyc": "^10.3.2",
    "style-loader": "^0.16.0",
    "tap-spec": "^4.1.1",
