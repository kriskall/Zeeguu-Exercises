{
  "name": "language_practice_platform",
  "version": "0.3.0",
  "description": "An open adaptive online language practice platform",
  "main": "main.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack",
    "production": "NODE_ENV=production webpack",
    "ci": "npm test",
    "test": "nyc babel-tape-runner test/*.js | tap-spec",
    "nsp": "nsp check --output checkstyle",
    "report": "nyc report --reporter=lcov",
    "posttest": "npm run nsp && npm run report",
    "coveralls": "npm run posttest && npm test && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "webpack -p --progress",
    "heroku-postbuild": "webpack -p --progress"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-tape-runner": "2.0.1",
    "coveralls": "2.13.1",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "0.11.2",
    "jquery": "3.4.0",
    "nsp": "2.8.1",
    "nyc": "11.9.0",
    "remove-accents": "0.4.2",
    "style-loader": "0.18.2",
    "tap-spec": "4.1.2",
    "tape": "4.9.1",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-cli": "2.1.5",
    "webpack-dev-server": "3.1.11"
  },
  "dependencies": {
    "jquery": "3.4.0",
    "mustache": "2.3.2",
    "page": "1.7.1",
    "sweetalert": "1.1.3"
  }
}
